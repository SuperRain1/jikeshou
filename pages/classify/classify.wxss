page{
  background: #fff;
}
.leftMenu{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  width: 180rpx;
  overflow-y:scroll;
  background: #f2f2f2;
}
.active{
  background: #fff;
  color: #ff4516!important;
}
.leftMenu view{
  height: 88rpx;
  line-height: 88rpx;
  color: #999999;
  font-size: 30rpx;
  padding-left: 26rpx;
}
.rightXq{
  position: fixed;
  right: 0;
  top: 0;
  bottom: 0;
  width: 560rpx;
  overflow-y:scroll;
}
.bz{
  color: #000000;
  font-size: 28rpx;
  text-align: center;
  margin: 30rpx 0;
}
.bz text{
  color: #ff4516;
  font-size: 32rpx;
}
.fl{
  overflow: hidden;
  width: 100%;
}
.fl view{
  width: 33%;
  margin-bottom: 30rpx;
  float: left;
}
.fl image{
  width: 112rpx;
  height: 112rpx;
  display: block;
  border-radius: 14rpx;
  margin: 0 auto;
}
.fl text{
  display: block;
  line-height: 68rpx;
  text-align: center;
  font-size: 26rpx;
}