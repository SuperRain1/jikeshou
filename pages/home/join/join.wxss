.type{
  display: flex;
  width: 100%;
  margin-top: 78rpx;
}
.type view{
  flex: 1;
}
.type image{
  width: 130rpx;
  height: 130rpx;
  display: block;
  margin: 0 auto;
}
.type view text{
  display: block;
  color: #cccccc;
  font-size: 30rpx;
  text-align: center;
  margin-top: 16rpx;
}
.active text{
  color: #ff4516!important;
}
.tab{
  width: 100%;
}
.tab view{
  width: 92%;
  margin: 0 auto;
  border-bottom: 1rpx solid #e4e4e4;
  height: 138rpx;
  line-height: 138rpx;
  overflow: hidden;
}
.tab label{
  float: left;
  font-size: 30rpx;
  margin-left: 4%;
}
.tab input{
  width: 66%;
  float: right;
  height: 138rpx;
  font-size: 30rpx;
  color: #000000;
}
.btn{
  width: 92%;
  background: #ff4516;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: white;
  margin: 288rpx auto 0;
  border-radius: 10rpx;
}