page{
  background: #e4e4e4;
}
.list{
  width: 92%;
  margin: 20rpx auto;
  background: white;
  overflow: hidden;
  border-radius: 14rpx;
}
.dd{
  font-size: 28rpx;
  padding: 40rpx 0 36rpx;
  color: #000000;
  margin-left: 4%;
}
.dd text{
  float: right;
  margin-right: 4%;
  font-size: 26rpx;
}
.time{
  font-size: 26rpx;
  padding-bottom:44rpx;
  margin-left: 4%;
  color: #5f5f5f;
}
.phone{
  background: #f7f7f7;
  line-height: 80rpx;
  color: #5f5f5f;
  font-size: 24rpx;
  padding-left: 4%;
 
}
.phone button{
  width: 142rpx;
  height: 50rpx;
  background: #ff4516;
  line-height: 50rpx;
  float: right;
  border-radius: 100rpx;
  color: white;
  font-size: 24rpx;
  margin-top: 15rpx;
  margin-right: 4%;
}
.pj{
  background: #f7f7f7;
  line-height: 80rpx;
  color: #ff4516;
  font-size: 24rpx;
  text-align: center;
}
.ypj{
  background: #f7f7f7;
  line-height: 80rpx;
  color: #cccccc;
  font-size: 24rpx;
  text-align: center;
}