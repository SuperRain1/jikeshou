page{
  background: #e4e4e4;
}
.box{
  width: 92%;
  background: white;
  margin: 20rpx auto 0;
  border-radius: 14rpx;
}
.suc{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto 80rpx;
  padding-top: 80rpx;
}
.h1{
  text-align: center;
  width: 100%;
  color: #000000;
  font-size: 40rpx;
  padding-bottom: 146rpx;
}
.x{
  display: block;
  width: 100%;
  position: fixed;
  top: 480rpx;
  left: 0;
  right: 0;
  height: 50rpx;
}
.h2{
  text-align: center;
  width: 100%;
  color: #5f5f5f;
  font-size: 28rpx;
}
.box view:last-child{
  padding: 8rpx 0 70rpx;
}
.btn{
  position: fixed;
  bottom: 84rpx;
  width: 92%;
  left: 4%;
  right: 4%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  color: white;
  background: #ff4516;
  font-size: 28rpx;
  border-radius: 10rpx;
}